{
    "compilerOptions": {
      "target": "es6",
      "lib": ["es6","es2017"],
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "jsx": "react",
      "allowJs": true,
      "sourceMap": true,
      "inlineSources": true,
      "types": ["node"],
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "strictNullChecks":false
    }
  }
  